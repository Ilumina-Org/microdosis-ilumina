{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/inventory/edit/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/inventory/new/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/inventory/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/inventory/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/orders/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/orders/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/calculate-shipping/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/check-stock/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/create-preference/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/districts/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/orders/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/orders/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/save-address/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/save-payment/?$",
			"dest": "_render"
		},
		{
			"src": "^/failure/?$",
			"dest": "_render"
		},
		{
			"src": "^/microdosis-package/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/pending/?$",
			"dest": "_render"
		},
		{
			"src": "^/success/?$",
			"dest": "_render"
		},
		{
			"src": "^/suscription/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}
---
import Layout from "../layouts/Layout.astro";
import Chatbot from "../components/ReactComponents/Chatbot";
import Products from "../components/Products.astro";
import Landing from "../components/ReactPages/Landing.tsx";
import Testimonials from "../components/ReactPages/Testimonials.tsx";
import About from "../components/ReactPages/About.tsx";
import FAQs from "../components/ReactPages/FAQs.tsx";
---

<Layout>
  <Landing id="inicio" client:load />
  <About id="about" />
  <Testimonials id="testimonios" client:load />
